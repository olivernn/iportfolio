<h1>Listing profiles</h1>

<table>
  <tr>
  </tr>

<% for profile in @profiles %>
  <tr>
    <td><%= link_to 'Show', [@user, profile] %></td>
    <td><%= link_to 'Edit', edit_user_profile_path(@user, profile) %></td>
    <td><%= link_to 'Destroy', [@user, profile], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New profile', new_user_profile_path(@user) %>

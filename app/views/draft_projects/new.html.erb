<h1>New project</h1>

<% form_for(@project, :url => {:controller => 'draft_projects', :action => 'create'}) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'projects/form', :locals => {:f => f} %>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', projects_path %>
